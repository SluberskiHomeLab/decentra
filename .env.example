# PostgreSQL Database Configuration
POSTGRES_DB=decentra
POSTGRES_USER=decentra
POSTGRES_PASSWORD=YOUR_SECURE_PASSWORD_HERE

# Server Configuration
# Format: postgresql://user:password@host:port/database
# Make sure the password in DATABASE_URL matches POSTGRES_PASSWORD above
DATABASE_URL=postgresql://decentra:YOUR_SECURE_PASSWORD_HERE@postgres:5432/decentra

# Encryption Configuration
# REQUIRED: Generate a strong encryption key for sensitive data encryption
# Generate a key using: python3 -c 'import secrets; print(secrets.token_urlsafe(32))'
DECENTRA_ENCRYPTION_KEY=YOUR_ENCRYPTION_KEY_HERE
